<br />
<div class="booyah-box col-10 offset-1">
  <h1>Add a new Chapter</h1>
  <%= simple_form_for @chapter, url: coach_section_chapters_path(current_section) do |f| %>
    <%= f.input :title %>
    <%= f.input :subtitle %>
    <%= f.input :video %>
    <br />
    <%= f.submit "Add the chapter" %>
    <br />
    <br />
  <% end %>
</div>