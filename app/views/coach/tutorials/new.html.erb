<br />
<div class="booyah-box col-10 offset-1">
  <h1>Start a new tutorial</h1>
   <%= simple_form_for @tutorial, url: coach_tutorials_path do |f| %>
    <%= f.input :title %>
    <%= f.select :System_type, ['Apple', 'Android'] %>
    <%= f.input :description %>
    <%= f.input :cost %>
    <%= f.input :image %>
    <br />
    <%= f.submit "Create", class: 'btn btn-primary' %>
    <br />
  <% end %>
  <br />
</div>


